   "source": [
    "from fastapi import FastAPI, File, UploadFile\n",
    "\n",
    "app = FastAPI()\n",
    "\n",
    "@app.post(\"/uploadfile/\")\n",
    "async def upload_file(file: UploadFile):\n",
    "    with open(file.filename, \"wb\") as f:\n",
    "        f.write(file.file.read())\n",
    "    return {\"filename\": file.filename}"
   ]
  }
